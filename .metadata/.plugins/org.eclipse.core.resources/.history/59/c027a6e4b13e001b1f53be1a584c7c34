package Day6;

import java.math.BigDecimal;
import java.util.Date;

public class Demo {

	public static void main(String[] args) {
		BirthDay b1 = new BirthDay(2000,1,1);
		BirthDay b2 = new BirthDay(2000,1,1);
		
		//System.out.println(b1==b2);//false   哈希值相等   @15db9742
		
		b1=b2;//地址
		new BirthDay(2020,1,1);//匿名对象    
		Date a = new Date(); 
		
		BigDecimal b = new BigDecimal(0.1);//0.1000000000000000055511151231257827021181583404541015625
		System.out.println(b);
		
		System.out.println(0.1 - 0.06);
	}
	
}
