package Day2;

public class case3 {
	public static void main(String[] args) {
		int[] arr_demo= {1,2,2,3,3,3,4,4,4,2,2,2};
		int max=arr_demo[0];
		int count = 0;
		int maxCount_k = arr_demo[0];
		int maxCount_v = 0;
		for (int i = 0; i < arr_demo.length; i++) {
			if(max<arr_demo[i]) {
				max=arr_demo[i];
			}
		}
		System.out.println(max);
		
		for (int i = 0; i < arr_demo.length; i++) {
			if(arr_demo[i]==3) {
				count++;
			}
		}
		System.out.println(count);
		
		
	}
}
