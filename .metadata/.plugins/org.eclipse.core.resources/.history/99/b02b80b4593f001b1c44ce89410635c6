package Day7;

public class Bike extends Vehicle{
	private String name;
	private double weight;
	
	@Override
	public void run() {
		// TODO Auto-generated method stub
		System.out.println("bike run");
	}

	@Override
	public void show() {
		// TODO Auto-generated method stub
		System.out.println(this.name);
		System.out.println(this.weight);
	}
	
}
