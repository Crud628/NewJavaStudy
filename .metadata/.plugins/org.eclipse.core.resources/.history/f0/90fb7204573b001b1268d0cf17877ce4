package Day4;

import java.util.Random;

public class case1_shuffle {

	public static void main(String[] args) {
		
		Random random=new Random();
		int temp=random.nextInt(53)+1;//1-54
		System.out.println(52/13);
		switch(temp/13) {
		
			case 0:
				break;
			case 1:
				break;
			case 2:
				break;
			case 3:
				break;
			case 4:
				break;
		}
		
	}

}
