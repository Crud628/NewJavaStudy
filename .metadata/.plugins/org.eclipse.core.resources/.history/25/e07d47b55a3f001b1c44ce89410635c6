public class VehicleTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Vehicle v = new Vehicle("A",100);
		v.show();
		v.run();
		System.out.println("-----------------上转型----------------------");
		//上转型
		Vehicle v2 = new Car("Car1",200);
		v2.show();
		v2.run();
		
		System.out.println("-----------------下转型----------------------");
	}

}
