package Day7Test;

public class Hero {
	private double skill1;
	private double skill2;
	private double skill3;
	public double getSkill1() {
		return skill1;
	}
	public void setSkill1(double skill1) {
		this.skill1 = skill1;
	}
	public double getSkill2() {
		return skill2;
	}
	public void setSkill2(double skill2) {
		this.skill2 = skill2;
	}
	public double getSkill3() {
		return skill3;
	}
	public void setSkill3(double skill3) {
		this.skill3 = skill3;
	}
	
	
}
